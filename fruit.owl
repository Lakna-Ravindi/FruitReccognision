<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/fruit_recommendation.owl#"
     xml:base="http://example.org/fruit_recommendation.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:Ontology rdf:about="http://example.org/fruit_recommendation.owl"/>

  <!-- === CLASSES === -->
  <owl:Class rdf:about="#User"/>
  <owl:Class rdf:about="#HealthCondition"/>
  <owl:Class rdf:about="#DietaryPreference"/>
  <owl:Class rdf:about="#FitnessGoal"/>
  <owl:Class rdf:about="#Fruit"/>
  <owl:Class rdf:about="#RipenessLevel"/>
  <owl:Class rdf:about="#Nutrient"/>
  <owl:Class rdf:about="#Recipe"/>
  <owl:Class rdf:about="#Recommendation"/>
  <owl:Class rdf:about="#CompatibilityFlag"/>

  <!-- Subclasses of Fruit -->
    <owl:Class rdf:about="#Apple">
        <rdfs:subClassOf rdf:resource="#Fruit"/>
    </owl:Class>
    <owl:Class rdf:about="#Banana">
        <rdfs:subClassOf rdf:resource="#Fruit"/>
    </owl:Class>
    <owl:Class rdf:about="#Mango">
        <rdfs:subClassOf rdf:resource="#Fruit"/>
    </owl:Class>
    <owl:Class rdf:about="#Orange">
        <rdfs:subClassOf rdf:resource="#Fruit"/>
    </owl:Class>
    <owl:Class rdf:about="#Dragonfruit">
        <rdfs:subClassOf rdf:resource="#Fruit"/>
    </owl:Class>
    <owl:Class rdf:about="#Strawberry">
        <rdfs:subClassOf rdf:resource="#Fruit"/>
    </owl:Class>

    <!-- Subclasses of RipenessLevel -->
    <owl:Class rdf:about="#Ripe">
        <rdfs:subClassOf rdf:resource="#RipenessLevel"/>
    </owl:Class>
    <owl:Class rdf:about="#Unripe">
        <rdfs:subClassOf rdf:resource="#RipenessLevel"/>
    </owl:Class>
    <owl:Class rdf:about="#Overripe">
        <rdfs:subClassOf rdf:resource="#RipenessLevel"/>
    </owl:Class>

  <!-- === OBJECT PROPERTIES === -->

  <owl:ObjectProperty rdf:about="#hasHealthCondition">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#HealthCondition"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasDietaryPreference">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#DietaryPreference"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasFitnessGoal">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#FitnessGoal"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#recommendsFruit">
    <rdfs:domain rdf:resource="#Recommendation"/>
    <rdfs:range rdf:resource="#Fruit"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasRipenessLevel">
    <rdfs:domain rdf:resource="#Fruit"/>
    <rdfs:range rdf:resource="#RipenessLevel"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#containsNutrient">
    <rdfs:domain rdf:resource="#Fruit"/>
    <rdfs:range rdf:resource="#Nutrient"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasRecipe">
    <rdfs:domain rdf:resource="#Fruit"/>
    <rdfs:range rdf:resource="#Recipe"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasCompatibilityFlag">
    <rdfs:domain rdf:resource="#Fruit"/>
    <rdfs:range rdf:resource="#CompatibilityFlag"/>
  </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="#hasRipenessLevel">
        <rdfs:domain rdf:resource="#Fruit"/>
        <rdfs:range rdf:resource="#RipenessLevel"/>
    </owl:ObjectProperty>

  <!-- Inverse -->
  <owl:ObjectProperty rdf:about="#isRecommendedTo">
    <owl:inverseOf rdf:resource="#recommendsFruit"/>
  </owl:ObjectProperty>

  <!-- === DATATYPE PROPERTIES === -->

  <owl:DatatypeProperty rdf:about="#age">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#gender">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
