    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#calories">
    <rdfs:domain rdf:resource="#Fruit"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#sugarContent">
    <rdfs:domain rdf:resource="#Fruit"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#vitaminC">
    <rdfs:domain rdf:resource="#Fruit"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#recipeName">
    <rdfs:domain rdf:resource="#Recipe"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#compatibilityNote">
    <rdfs:domain rdf:resource="#CompatibilityFlag"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

